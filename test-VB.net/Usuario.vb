Imports System

Imports System.Text


Public Class Usuario

    'Adiciona usuario
    Shared Function Add_users() As RepCid

        Dim rep As RepCid
        Dim status, gravou As New Boolean


        If rep Is Nothing Then

            rep = New RepCid()

            rep = Conexao.ConectaRep_iDClass()
            'Método GravarUsuario(Long,string,integer,string,string,integer,integer,boolean)
            status = rep.GravarUsuario(85393853933, "Mauro - Teste", 125, "12345", "121", 0, 1, gravou)

            If status = True Then

                Console.WriteLine("Gravou")
                rep.Desconectar()
            Else

                Console.WriteLine("Não gravou")

            End If

        End If

    End Function


